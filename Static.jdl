entity  People {
  firstname String
  lastname String
}

entity Address {
  street String
  city String
  state String
  zip String
}

service People with serviceClass

dto People with mapstruct

relationship ManyToOne {
  Address{people} to People{address}
}

entity Warehouse {
  name String
  address String
}

entity Product {
  name String
  description String
  price BigDecimal
}

relationship  ManyToMany {
  Warehouse{products} to Product{warehouses}
}
